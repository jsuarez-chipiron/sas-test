            <group>Sykes</group>
        </sharedTo>
        <criteriaItems>
            <field>RecordTypeId</field>
            <operation>equals</operation>
            <value>Information,Support,Feedback</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>SAS_Cases</fullName>
        <accessLevel>Edit</accessLevel>
