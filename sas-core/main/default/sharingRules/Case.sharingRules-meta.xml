<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Sharing_Cases_with_all_the_vendor</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share read/write access for cases to all vendors</description>
        <label>Sharing Cases with all the vendor</label>
        <sharedTo>
            <group>All_Vendors</group>
        </sharedTo>
        <criteriaItems>
            <field>Team__c</field>
            <operation>notEqual</operation>
            <value>Feedback,Fraud Investigation</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingCriteriaRules>
        <fullName>Sharing_cases_with_SAS_people</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Shares all the cases with SAS people</description>
        <label>Sharing cases with SAS people</label>
        <sharedTo>
            <roleAndSubordinatesInternal>SAS</roleAndSubordinatesInternal>
        </sharedTo>
        <criteriaItems>
            <field>CaseNumber</field>
            <operation>notEqual</operation>
            <value>00000000</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>Liability_Cases_to_Liability_team</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Liability Cases to Liability team</label>
        <sharedTo>
            <group>Liability</group>
        </sharedTo>
        <sharedFrom>
            <group>Liability</group>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
