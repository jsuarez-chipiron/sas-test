<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Sharing_Cases_with_all_the_vendor</fullName>
        <accessLevel>Edit</accessLevel>
        <description>Share read/write access for cases to all vendors</description>
        <label>Sharing Cases with all the vendor</label>
        <sharedTo>
            <group>All_Vendors</group>
        </sharedTo>
        <booleanFilter>1 AND ( 2 OR 3)</booleanFilter>
        <criteriaItems>
            <field>Department__c</field>
            <operation>notEqual</operation>
            <value>SAS Operating Center</value>
        </criteriaItems>
        <criteriaItems>
            <field>Team__c</field>
            <operation>notEqual</operation>
            <value>Feedback</value>
        </criteriaItems>
        <criteriaItems>
            <field>Team__c</field>
            <operation>notEqual</operation>
            <value>Fraud Investigation</value>
        </criteriaItems>
    </sharingCriteriaRules>
    <sharingOwnerRules>
        <fullName>Liability_Cases_to_Liability_team</fullName>
        <accessLevel>Edit</accessLevel>
        <label>Liability Cases to Liability team</label>
        <sharedTo>
            <group>Liability</group>
        </sharedTo>
        <sharedFrom>
            <group>Liability</group>
        </sharedFrom>
    </sharingOwnerRules>
    <sharingOwnerRules>
        <fullName>SAS_Cases</fullName>
        <accessLevel>Edit</accessLevel>
        <description>SAS can see all Cases</description>
        <label>SAS Cases</label>
        <sharedTo>
            <group>SAS</group>
        </sharedTo>
        <sharedFrom>
            <allInternalUsers/>
        </sharedFrom>
    </sharingOwnerRules>
</SharingRules>
