    <formulas>
        <name>chequeMessage</name>
        <dataType>String</dataType>
        <expression>IF({!incomingRecord.RecordType.Name} = &apos;Cheque&apos;, &quot;Cheques always go to Control&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>controlReasonDetail</name>
        <dataType>String</dataType>
        <expression> IF({!incomingRecord.RecordType.Name} = &quot;Monetary&quot;, &quot;Bank account number&quot;, IF({!incomingRecord.RecordType.Name} = &quot;Voucher&quot;, &quot;Voucher Email&quot;, &quot;EB number&quot;))</expression>
    </formulas>
    <formulas>
        <name>highAmountMessage</name>
        <dataType>String</dataType>
        <expression>IF({!highAmountFlag} = true,&quot;High amount settlement&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>inactiveAccountMessage</name>
        <dataType>String</dataType>
        <expression>IF({!inactiveAccountFlag}, &quot;Settlement was sent to Control as the account connected to the Claim Firm is inactive&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>last12months</name>
        <dataType>Date</dataType>
        <expression>ADDMONTHS(TODAY(), -12)</expression>
    </formulas>
    <formulas>
        <name>numberSettlementsMessage</name>
        <dataType>String</dataType>
        <expression>IF({!numberOfSettlements} &gt;= 3 , {!controlReasonMessage} , &quot;&quot;)</expression>
    </formulas>
