    <decisions>
        <name>Are_all_Settlements_valid</name>
        <label>Are all Settlements valid?</label>
        <locationX>710</locationX>
        <locationY>1670</locationY>
        <defaultConnector>
            <targetReference>errorScreenInvalidSettlements</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>ValidSettlements</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>numberOfInvalidSettlements</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>submitScreen</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <name>Case_can_be_submitted_for_Approval</name>
        <label>Case can be submitted for Approval</label>
        <locationX>578</locationX>
        <locationY>1310</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>No_email_draft_exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Email_Draft_for_this_Case</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Case_Information.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Customer_Claim</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>errorScreenEmail</targetReference>
            </connector>
            <label>No email draft exists</label>
        </rules>
        <rules>
            <name>No_Settlements_to_approve</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>numberOfSettlementsNeedingApproval</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>errorScreenNoSettlements</targetReference>
            </connector>
            <label>No Settlements to approve</label>
        </rules>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Get_Case_Information.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>In Progress</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>numberOfSettlementsNeedingApproval</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Settlements_for_this_Case</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
