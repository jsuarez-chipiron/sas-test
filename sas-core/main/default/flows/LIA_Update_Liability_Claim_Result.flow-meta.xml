    <decisions>
        <name>Capture_Paid_Amounts</name>
        <label>Capture Paid Amounts</label>
        <locationX>282</locationX>
        <locationY>362</locationY>
        <defaultConnector>
            <targetReference>Set_Paid_Amounts_to_0</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No paid amounts</defaultConnectorLabel>
        <rules>
            <name>Update_Payments_Manually</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>Result</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Negative</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Result</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Partly</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Result</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Settlement</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Result</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Default Judgement</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Paid_Amounts_Update</targetReference>
            </connector>
            <label>Ask for Paid Amounts</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_Settlement_Reason_Applicable</name>
        <label>Is Settlement Reason Applicable?</label>
        <locationX>691</locationX>
        <locationY>660</locationY>
        <defaultConnector>
            <targetReference>Set_Settlement_Reason_to_N_A</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Settlement Reason does not apply</defaultConnectorLabel>
        <rules>
            <name>Settlement_Reason_applies</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Result</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Settlement</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Input_Settlement_Reason_To_Settlement_Reason</targetReference>
            </connector>
            <label>Settlement Reason applies</label>
        </rules>
    </decisions>
