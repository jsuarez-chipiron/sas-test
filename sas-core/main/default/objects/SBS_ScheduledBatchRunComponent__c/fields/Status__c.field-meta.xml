<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status__c</fullName>
    <description>The status of this batch process within the scheduled batch run. Batches starts as scheduled and should move from started to success. In cases where failure occurs when initializing the batch the batches receive the init error status and in the case where one or several of its batches fails the status is set to fail.</description>
    <externalId>false</externalId>
    <label>Status</label>
    <required>true</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>Scheduled</fullName>
                <default>false</default>
                <label>Scheduled</label>
            </value>
            <value>
                <fullName>Started</fullName>
                <default>false</default>
                <label>Started</label>
            </value>
            <value>
                <fullName>Success</fullName>
                <default>false</default>
                <label>Success</label>
            </value>
            <value>
                <fullName>Fail</fullName>
                <default>false</default>
                <label>Fail</label>
            </value>
            <value>
                <fullName>Init Error</fullName>
                <default>false</default>
                <label>Init Error</label>
            </value>
        </valueSetDefinition>
    </valueSet>
</CustomField>
